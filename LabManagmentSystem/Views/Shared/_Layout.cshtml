    <!-- Core JS files -->

    <script>
        function completed(response) {

            response = response.responseJSON;
            if (response.status == 1) {
                swal({
                    icon: 'success',
                    text: 'Saved'
                })

                if (response.create == "true") {
                    document.getElementById("createform").reset();
                }

            }
            else {
                swal({
                    icon: 'error',
                    text: response.message
                })
            }
        }


    </script>
